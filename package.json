{
  "name": "chef-socket",
  "version": "2.1.0",
  "keywords": [
    "chef-js",
    "server",
    "static-serve",
    "http-server",
    "https-server",
    "nodejs-server",
    "express",
    "spa",
    "nodejs",
    "javascript",
    "cli",
    "typescript",
    "socket.io",
    "websockets"

  "dependencies": {
    "chef-core": "^3.0.2",
    "express": "^4.18.2",
    "socket.io": "^4.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.0",
    "jest": "^29.5.0",
